{
  "MongoDB:uuid": {
    "id":"String[>=5]'superParkour-1'", //<-- durch Admin erstellt
    "comment":"String[0|>=0]'ein schwieriger Parkour'", //<-- durch Admin erstellt
    "pose": {
      "start":"String[>=5]'Ausgangslage: Person mittig vor Host,..'"
    },
    "exercises": [ //<-- Info: auch NESTURES müssen in einem Parkour definiert werden
        {
          "mutation": {
            "uuid":"MUTATION.uuid/collection.mutations.json" //<-- durch Admin erstellt
          },
          "signal": { //<-- betrifft _OPTIONAL-1 und _OPTIONAL-2
            "beep": true, //<-- lässt das Handschuh-Paar (die aktive Hand) piepsen
            "ignoreNestures": true //<-- setzt das Signal für NESTURES aus
          }
        }
      ]
    }
  }
}
